{
    "apps": [{
      "name": "data-parse",
      "script": "./lib/connect",
      "cwd": "./",
      "exec_mode": "fork",
      "instances": 1,
      "autorestart": true,
      "max_restarts":15,
      "min_uptime": "30s",
      "node_args": "--max-old-space-size=65500",
      "out_file": "/tmp/data-parse.log",
      "error_file": "/tmp/data-parse.log",
      "combine_logs": true,
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      }
    }]
  }
  